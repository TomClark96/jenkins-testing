pipeline {
    agent any
    parameters {
        string(name: 'ucdVersionDescription', description: 'Please add a description')
    }
    stages {
        stage('Echo Parms') {
            steps {
                echo "${params.ucdVersionDescription}"
                }
            }
        stage('Create JSON') {
            steps {
                script {
                    def valueFromBuild = "${params.ucdVersionDescription}"
                    def jsonString = '{"ucdDescription": "${valueFromBuild}" }'
                    def jsonObj = readJSON text: jsonString

                    sh "echo ${jsonObj.ucdDescription}"
                }
            }
        }
    }
}
