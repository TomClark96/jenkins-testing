pipeline {
    agent any
    parameters {
        string(name: 'ucdVersionDescription', description: 'Please add a description')
    }
    stages {
        stage('Echo Parms') {
            steps {
                echo "${params.ucdVersionDescription}"
                }
            }
        stage('Create JSON') {
            steps {
                script {
                    echo "{"ucdDescription": "$(params.ucdVersionDescription)}"
                }
            }
        }
    }
}
