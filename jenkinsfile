pipeline {
    agent any
    parameters {
        string(name: 'ucdVersionDescription', description: 'Please add a description')
    }
    stages {
        stage('Echo Parms') {
            steps {
                echo "${params.ucdVersionDescription}"
                }
            }
        stage('Create JSON') {
            steps {
                script {
                    def jsonString = "{"ucdDescription":"${params.ucdVersionDescription}"}"
                    def jsonObj = readJSON text: jsonString

                    assert props['ucdDescription'] == "${params.ucdVersionDescription}"
                    sh "echo ${jsonObj.ucdDescription}"
                }
            }
        }
    }
}
